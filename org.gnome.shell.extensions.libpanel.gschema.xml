<!-- Run this when edited: `glib-compile-schemas .` -->
<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema id="org.gnome.shell.extensions.libpanel" path="/org/gnome/shell/extensions/libpanel/">
    <!--
      The outermost dictionnary is mapping of monitor name (as defined by Dash To Panel) to grid layout. The innermost one is mapping of panel id to position in the gris.
    --> 
    <key name="layout" type="a{ia{s(ii)}}">
      <default>{}</default>
    </key>
    <key name="padding-enabled" type="b">
      <default>false</default>
    </key>
    <key name="padding" type="i">
      <default>18</default>
    </key>
    <key name="row-spacing-enabled" type="b">
      <default>false</default>
    </key>
    <key name="row-spacing" type="i">
      <default>12</default>
    </key>
    <key name="column-spacing-enabled" type="b">
      <default>false</default>
    </key>
    <key name="column-spacing" type="i">
      <default>12</default>
    </key>
  </schema>
</schemalist>